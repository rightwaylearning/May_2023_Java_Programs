package default_values;

public class Controller {

	 
	 
     public static void main(String[] args){

     
    	 byte b = 4;
    	 
    	 int i = b;
    	 
    	 //-----------------------------
    	 
    	 int x = 9;
    	 
        	 
    	 float f = x; // 9.0
    	 
    	 double d = b;
    	 
    	 //-----------------------------------------------
    	 
    	 char c = 'A';  // 65
    	 
    	 int h = c;
    	 
    	 System.out.println(h);

     }
}
