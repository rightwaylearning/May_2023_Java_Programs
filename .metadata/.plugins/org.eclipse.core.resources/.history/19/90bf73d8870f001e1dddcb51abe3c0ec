package var_arg_method;

public class Cal {

	
	void sum(int... num) {
		int s = 0;
		for(int i =0 ; i < num.length ; i++) {
			s =s + num[i];
		}
		System.out.println(s);
	}
	
	void printStudentName(String... names) {
		
		for(int i= 0 ; i < names.length ; i++) {
			System.out.println(names[i]);
		}
		
	}
}
