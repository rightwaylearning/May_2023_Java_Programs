package forloop.examples;

public class Test {

	public static void main(String[] args) {
		
		int[] arr = new int[5];
		
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		arr[3] = 40;
		arr[4] = 50;
		
		
		 int i = 0;
		System.out.println(arr[i]);
		 i = i +1;
		System.out.println(arr[i]);
		 i = i + 1;
		
		System.out.println(arr[i]);
		 i = i +1;
		System.out.println(arr[i]);
		
		 i = i + 1;
		System.out.println(arr[i]);
		
		
		
		
		
	}
}
