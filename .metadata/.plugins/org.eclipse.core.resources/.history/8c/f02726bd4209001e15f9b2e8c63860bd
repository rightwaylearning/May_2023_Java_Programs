package reference.type.array;

public class ViewClassRoomInfo {

	
	static void printInfo(ClassRoom room) {
	
		System.out.println(room.classTeacherName);
		System.out.println(room.std);
		
		System.out.println(room.students[0].name);
		System.out.println(room.students[0].rollNumber);
		
		System.out.println(room.students[1].name);
		System.out.println(room.students[1].rollNumber);
		
		System.out.println(room.students[2].name);
		System.out.println(room.students[2].rollNumber);
		
	}
}

